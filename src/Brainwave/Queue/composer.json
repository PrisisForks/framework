{
    "name"       : "brainwave/queue",
    "type"       : "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords"   : ["narrowspark", "brainwave", "queue", "symfony"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/n-brainwave/queue",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                : ">=5.4.0",
        "brainwave/cotracts"                 : "self.version",
        "brainwave/events"                   : "self.version",
        "nesbot/carbon"                      : "~1.19"
    },
    "require-dev": {
        "aws/aws-sdk-php"                    : "~2.6",
        "predis/predis"                      : "~0.8",
        "pda/pheanstalk"                     : "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Brainwave\\Queue\\"             : ""
        }
    },
    "autoload-dev": {
        "psr-4": { "Brainwave\\Queue\\Test\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                     : "0.10-dev"
        }
    },
    "minimum-stability"                      : "dev",
    "prefer-stable"                          : true
}
